# EXAMPLE FILE - Copy to application-secrets.properties and fill in your credentials
# DO NOT put actual credentials in this example file as it will be committed to Git

# Database Credentials (used by ${secrets.database.password})
secrets.database.password=your_supabase_database_password_here

# Email Configuration (used by ${secrets.mail.username} and ${secrets.mail.password})
secrets.mail.username=your_email@gmail.com
secrets.mail.password=your_gmail_app_password_here

# Spring Boot Configuration
spring.datasource.password=your_supabase_database_password_here

spring.mail.password=your_gmail_app_password_here

# Additional Configuration Options (uncomment and configure as needed)
# jwt.secret=your_jwt_secret_here
# api.key=your_additional_api_key_here 