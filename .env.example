# Copy this file to .env and fill in your actual credentials
# DO NOT commit .env file to Git!

# Database Configuration (Supabase)
SPRING_DATASOURCE_URL=jdbc:postgresql://your-supabase-host:6543/postgres?user=your-user&reWriteBatchedInserts=true
SPRING_DATASOURCE_USERNAME=your_supabase_username
SPRING_DATASOURCE_PASSWORD=your_supabase_password
DB_PASSWORD=your_supabase_password

# API Keys
HUGGINGFACE_API_KEY=hf_your_huggingface_api_key_here
RAPIDAPI_YOUTUBE_KEY=your_rapidapi_youtube_key_here
DEEPL_API_KEY=your_deepl_api_key_here
STABILITY_AI_KEY=sk-your-stability-ai-key-here

# Mail Configuration (Gmail)
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your_email_username_here
MAIL_PASSWORD=your_gmail_app_password_here
